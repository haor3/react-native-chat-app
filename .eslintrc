{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "plugin:jest/recommended"],
  "plugins": ["babel", "import", "jsx-a11y", "react", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "constructor-super": "warn",
    "no-console": "warn",
    "no-const-assign": "warn",
    "no-debugger": "error",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "valid-typeof": "warn"
  }
}
